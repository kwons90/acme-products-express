{
  "name": "beaver",
  "version": "1.0.0",
  "description": "This is a \"boiler plate\". Its a foundation to build apps upon.",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "start": "NODE_ENV=\"production\" node server/index.js",
    "start:client": "webpack-dev-server --open",
    "start:server": "node-dev --respawn server/index.js",
    "build": "webpack --mode=\"production\"",
    "build:watch": "webpack --watch",
    "lint": "eslint client server tests",
    "format:client": "prettier-eslint --write --single-quote --semi \"$PWD/client/**/*.js\"",
    "format:tests": "prettier-eslint --write --single-quote --semi \"$PWD/tests/**/*.js\"",
    "format:server": "prettier-eslint --write --single-quote --semi \"$PWD/server/**/*.js\"",
    "format": "npm run format:client && npm run format:tests && npm run format:server"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "node-dev": "^4.0.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "keywords": [],
  "author": "Eliot Szwajkowski (eliotpszw@gmail.com)",
  "license": "ISC"
}
